<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= current_page.data.title || "Miguel Ramos Portfolio" %></title>
    <%= stylesheet_link_tag "normalize", "styles" %>
</head>
<body>
    <div class="main">
        <div id="mobile-feed">
            <div id="x">X</div>
        </div>
        <div class="header">
          <div class="header-contents">
            <div class="title">
               <h1> MIGUEL RAMOS</h1>
               <h2> Front-End Web Developer </h2>
            </div>
            <div class="feed-init">
                <div class="feed-btn">
                    <img src="images/feed.svg" />
                </div>
            </div>
        </div>
    </div>
    <div id="container">
        <%= partial "nav" %>
        <%= yield %>
  </div>
</div>

<div class="sidebar">

</div>




<%= javascript_include_tag  "pjax", "main" %>
</body>
</html>
